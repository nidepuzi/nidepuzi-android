<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="SwipeBackLayoutStyle" format="reference"/>

    <declare-styleable name="SwipeBackLayout">
        <attr name="edge_size" format="dimension"/>
        <attr name="edge_flag">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
            <enum name="bottom" value="2"/>
            <enum name="all" value="3"/>
        </attr>
        <attr name="shadow_left" format="reference"/>
        <attr name="shadow_right" format="reference"/>
        <attr name="shadow_bottom" format="reference"/>
    </declare-styleable>

    <declare-styleable name="BaseTitleView">
        <attr name="title" format="string"/>
        <attr name="show_finish" format="boolean"/>
        <attr name="alpha" format="float"/>
        <attr name="back_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="MessageItemView">
        <attr name="message_title" format="string"/>
        <attr name="message_desc" format="string"/>
        <attr name="message_time" format="string"/>
        <attr name="message_icon" format="reference"/>
    </declare-styleable>
</resources>